          }
                    },
                    "$notification.remote": {
                        "type": "$util.banner",
                        "options": {
                            "title": "Message",
                            "description": "{{JSON.stringify($jason)}}"
                        }
                    }
                }
            }
    },
    "body": {
      "background": {
        "type": "html",
        "url": "https://edenkoveshi.github.io/amiad-porat/",
        "action": {
          "trigger": "visit"
        },
        {
        
      }
      }
    }
  }
}
